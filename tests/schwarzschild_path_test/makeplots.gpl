set term pdf

set output 'around_kt.pdf'
set xlabel 'theta/pi'
set ylabel 'k^t'
plot 'around.dat' u (atan2($2,$1)/3.14159):8 w l title "Sedonu", 1.732

set output 'around_r_diff.pdf'
set xlabel 'theta/pi'
set ylabel 'r-rs'
plot 'around.dat' u (atan2($2,$1)/3.14159):(sqrt($1*$1+$2*$2)-1.5) w l title "Sedonu", 1.5

set output 'radial_kt.pdf'
set xlabel 'r/rs'
set ylabel 'n_{mu} k^{mu}'
plot 'radial.dat' u 1:($8*sqrt(1-1/sqrt($1*$1+$2*$2))) w l title "Sedonu", 3*sqrt(1-1/1.5)*sqrt((1-1/1.5)/(1-1/x))

set size ratio -1
set output 'around_r.pdf'
set xlabel 'x'
set ylabel 'y'
plot 'around.dat' u 1:2 w l title "Sedonu"

