set term pdf

set output 'around_kt.pdf'
set xlabel 'theta/pi'
set ylabel 'k^t'
plot 'around.dat' u (atan2($2,$1)/3.14159):9 w l title "Sedonu", 1.732

set output 'around_r.pdf'
set xlabel 'theta/pi'
set ylabel 'r/rs'
plot 'around.dat' u (atan2($2,$1)/3.14159):(sqrt($1*$1+$2*$2)) w l title "Sedonu", 1.5

set output 'radial_kt.pdf'
set xlabel 'r/rs'
set ylabel 'n_{mu} k^{mu}'
plot 'radial.dat' u 1:($9*sqrt(1-1/sqrt($1*$1+$2*$2))) w l title "Sedonu", 3*sqrt(1-1/1.5)*sqrt((1-1/1.5)/(1-1/x))