Cell[BoxData[
 RowBox[{
  RowBox[{"phil", "[", 
   RowBox[{"l_", ",", "\[Omega]a_", ",", "\[Omega]b_", ",", "Ee_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "y", ",", "AA", ",", "BB", ",", "CC", ",", "DD", ",", "\[Mu]0", ",", "f",
       ",", "integrand", ",", "musub", ",", "xsub", ",", "dmudx", ",", "a", 
      ",", "b", ",", "c", ",", "d", ",", "tmp0", ",", "tmp1", ",", "tmp2", 
      ",", "\[Mu]", ",", "x", ",", "J1", ",", "J2", ",", "phi"}], "}"}], ",", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
      RowBox[{"Equations", " ", "C57"}], "-", 
      RowBox[{"C61", " ", "and", " ", "text", " ", "above", " ", "C66"}]}], 
     " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"y", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"\[Omega]b", "^", "2"}], " ", 
         RowBox[{"\[Omega]a", "^", "2"}], " ", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"1", "-", "\[Mu]"}], ")"}], "^", "2"}]}], ")"}], "/", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"\[Omega]b", "^", "2"}], "+", 
          RowBox[{"\[Omega]a", "^", "2"}], "+", 
          RowBox[{"2", "\[Omega]a", " ", "\[Omega]b", " ", "\[Mu]"}]}], ")"}],
         "^", 
        RowBox[{"(", 
         RowBox[{"5", "/", "2"}], ")"}]}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"AA", "=", 
      RowBox[{
       RowBox[{"\[Omega]b", "^", "2"}], " ", "y", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"\[Omega]b", "+", 
            RowBox[{"\[Omega]a", " ", "\[Mu]"}]}], ")"}], "^", "2"}], "-", 
         RowBox[{
          RowBox[{"1", "/", "2"}], " ", 
          RowBox[{"\[Omega]a", "^", "2"}], " ", 
          RowBox[{"(", 
           RowBox[{"1", "-", 
            RowBox[{"\[Mu]", "^", "2"}]}], ")"}]}]}], ")"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"BB", "=", 
      RowBox[{"\[Omega]b", " ", "y", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"-", "2"}], 
          RowBox[{"\[Omega]b", "^", "2"}]}], "+", 
         RowBox[{"\[Omega]a", "^", "2"}], "+", 
         RowBox[{"3", "\[Omega]a", " ", "\[Omega]b"}], "-", 
         RowBox[{"\[Omega]a", " ", "\[Omega]b", " ", "\[Mu]"}], "+", 
         RowBox[{"3", 
          RowBox[{"\[Omega]a", "^", "2"}], " ", "\[Mu]"}]}], ")"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"CC", "=", 
      RowBox[{"y", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"\[Omega]b", "^", "2"}], "+", 
         RowBox[{"\[Omega]a", "^", "2"}], "-", 
         RowBox[{"3", "\[Omega]a", " ", "\[Omega]b"}], " ", "-", " ", 
         RowBox[{"\[Omega]a", " ", "\[Omega]b", " ", "\[Mu]"}]}], ")"}]}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"DD", "=", 
      RowBox[{"4", 
       RowBox[{"Ee", "^", "2"}], 
       RowBox[{"\[Omega]a", "^", "2"}], 
       RowBox[{"(", 
        RowBox[{"1", "-", 
         RowBox[{"\[Mu]", "^", "2"}]}], ")"}], "\[Omega]a", " ", "\[Omega]b", 
       
       RowBox[{"(", 
        RowBox[{"1", "-", "\[Mu]"}], ")"}], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"-", "2"}], 
          RowBox[{"Ee", "^", "2"}]}], "+", 
         RowBox[{"2", "Ee", 
          RowBox[{"(", 
           RowBox[{"\[Omega]a", "+", "\[Omega]b"}], ")"}]}], "-", 
         RowBox[{"\[Omega]a", " ", "\[Omega]b", 
          RowBox[{"(", 
           RowBox[{"1", "-", "\[Mu]"}], ")"}]}]}], ")"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"\[Mu]0", "=", 
      RowBox[{"1", "-", 
       RowBox[{"2", "Ee", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"\[Omega]a", "+", "\[Omega]b", "-", "Ee"}], ")"}], "/", 
         RowBox[{"(", 
          RowBox[{"\[Omega]a", " ", "\[Omega]b"}], ")"}]}]}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"f", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"AA", "+", 
         RowBox[{"BB", " ", "Ee"}], "+", 
         RowBox[{"CC", " ", 
          RowBox[{"Ee", "^", "2"}]}]}], ")"}], 
       RowBox[{"LegendreP", "[", 
        RowBox[{"l", ",", "\[Mu]"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
       RowBox[{
       "Substitutions", " ", "to", " ", "make", " ", "the", " ", "integral", 
        " ", "easier"}], " ", "-", " ", 
       RowBox[{
       "one", " ", "is", " ", "inverse", " ", "of", " ", "the", " ", 
        "other"}]}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"musub", " ", "=", " ", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"x", "^", "2"}], " ", "-", " ", 
         RowBox[{"\[Omega]b", "^", "2"}], " ", "-", " ", 
         RowBox[{"\[Omega]a", "^", "2"}]}], ")"}], "/", 
       RowBox[{"(", 
        RowBox[{"2", " ", "\[Omega]b", " ", "\[Omega]a"}], ")"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"xsub", " ", "=", " ", 
      RowBox[{"Sqrt", "[", 
       RowBox[{
        RowBox[{"\[Omega]b", "^", "2"}], " ", "+", " ", 
        RowBox[{"\[Omega]a", "^", "2"}], " ", "+", " ", 
        RowBox[{"2", " ", "\[Omega]b", " ", "\[Omega]a", " ", "\[Mu]"}]}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"dmudx", "=", 
      RowBox[{"D", "[", 
       RowBox[{"musub", ",", "x"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
